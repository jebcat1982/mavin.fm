= form_tag '/music' do
  - if @music.errors.any?
    #error_explanation
      %h2= "#{pluralize(@music.errors.count, "error")} prohibited this from being saved:"
      %ul
        - @music.errors.full_messages.each do |msg|
          %li= msg

  .field
    = label_tag 'url'
    = text_field_tag 'url'
  .field
    = label_tag 'raw_tags'
    = text_field_tag 'raw_tags'
  .actions
    = submit_tag 'Save'
