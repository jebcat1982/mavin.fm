= form_for @playlist_track do |f|
  -if @playlist_track.errors.any?
    #error_explanation
      %h2= "#{pluralize(@playlist_track.errors.count, "error")} prohibited this playlist_track from being saved:"
      %ul
        - @playlist_track.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :playlist_id
    = f.number_field :playlist_id
  .field
    = f.label :track_id
    = f.number_field :track_id
  .actions
    = f.submit 'Save'
